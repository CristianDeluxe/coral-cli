{
  "name": "@coralproject/coral-cli",
  "description": "CLI to interact with Coral",
  "version": "0.1.0",
  "author": "Wyatt Johnson @wyattjoh",
  "bin": {
    "coral-cli": "./bin/run"
  },
  "bugs": "https://github.com/coralproject/coral-cli/issues",
  "dependencies": {
    "@coralproject/coral-cli-command": "^0.1.0",
    "@heroku-cli/color": "^1.1.14",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "@oclif/plugin-not-found": "^1.2.3",
    "@oclif/plugin-plugins": "^1.7.8",
    "cli-ux": "^5.3.2",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.15",
    "netrc-parser": "^3.1.6"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/lodash": "^4.14.139",
    "@types/node": "^10.14.18",
    "@types/supports-color": "^5.3.0",
    "globby": "^10.0.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.3.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.6.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/coralproject/coral-cli",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "coral-cli",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins"
    ],
    "topics": {
      "story": {
        "description": "manage stories on Coral"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": "coralproject/coral-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p . -t stylish",
    "pack": "oclif-dev pack",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "tsc -p . --noEmit",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
